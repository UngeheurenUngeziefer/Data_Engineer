{
	"name": "LineItem Transformed to Azure SQL",
	"properties": {
		"activities": [
			{
				"name": "Transform Data",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "LineItemCleanFlow",
						"type": "DataFlowReference",
						"datasetParameters": {
							"source1": {
								"fileName": "h_lineitem_part2.csv"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			},
			{
				"name": "Copy Data to Azure SQL",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Transform Data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"ordinal": 0
								},
								"sink": {
									"name": "orderkey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"ordinal": 1
								},
								"sink": {
									"name": "partkey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"ordinal": 2
								},
								"sink": {
									"name": "suppkey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"ordinal": 3
								},
								"sink": {
									"name": "linenumber",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"ordinal": 4
								},
								"sink": {
									"name": "quantity",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"sink": {
									"name": "extendedprice",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 2,
									"precision": 10
								}
							},
							{
								"sink": {
									"name": "discount",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 2,
									"precision": 10
								}
							},
							{
								"sink": {
									"name": "tax",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 2,
									"precision": 10
								}
							},
							{
								"sink": {
									"name": "returnflag",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"ordinal": 9
								},
								"sink": {
									"name": "linestatus",
									"type": "Boolean",
									"physicalType": "bit"
								}
							},
							{
								"sink": {
									"name": "shipdate",
									"type": "DateTime",
									"physicalType": "datetime2"
								}
							},
							{
								"sink": {
									"name": "commitdate",
									"type": "DateTime",
									"physicalType": "datetime2"
								}
							},
							{
								"sink": {
									"name": "receiptdate",
									"type": "DateTime",
									"physicalType": "datetime2"
								}
							},
							{
								"sink": {
									"name": "shipinstruct",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"sink": {
									"name": "shipmode",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"ordinal": 15
								},
								"sink": {
									"name": "comment",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "lineitemFinalCSV",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlOutputTable",
						"type": "DatasetReference",
						"parameters": {
							"tableName": "lineitem"
						}
					}
				]
			}
		],
		"annotations": []
	}
}