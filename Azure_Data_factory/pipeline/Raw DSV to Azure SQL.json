{
	"name": "Raw DSV to Azure SQL",
	"properties": {
		"activities": [
			{
				"name": "DSV to Azure SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "S_SUPPKEY",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "suppkey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "S_NAME",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "name",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "S_ADDRESS",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "address",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "S_NATIONKEY",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "nationkey",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "S_PHONE",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "phone",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "S_ACCTBAL",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "acctbal",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 2,
									"precision": 10
								}
							},
							{
								"source": {
									"name": "S_COMMENT",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "comment",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "rawInputDSV",
						"type": "DatasetReference",
						"parameters": {
							"fileName": "h_supplier.dsv"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlOutputTable",
						"type": "DatasetReference",
						"parameters": {
							"tableName": "supplier"
						}
					}
				]
			}
		],
		"annotations": []
	}
}